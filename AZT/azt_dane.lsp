(defun zmiana_warstw_advance ()
  (setq azt_selekcja_as_yellow (ssget "X" (list (cons 62 2))))
  (setq azt_selekcja_as_axis (ssget "X" (list (cons 62 6) (cons 8 "Beams"))))
  (setq azt_selekcja_as_grids (ssget "X" (list (cons 62 6) (cons 8 "Grids"))))
  (setq azt_selekcja_as_cyan (ssget "X" (list (cons 62 4))))
  (setq azt_selekcja_as_green (ssget "X" (list (cons 62 3))))
  (setq azt_selekcja_as_8 (ssget "X" (list (cons 62 8))))
  (setq azt_selekcja_as_254 (ssget "X" (list (cons 62 254))))
  (command "_chprop" azt_selekcja_as_yellow "" "_LT" "ByLayer" "_C" "ByLayer" "_LA" "S_VIEW" "")
  (command "_chprop" azt_selekcja_as_axis "" "_LT" "ByLayer" "_C" "ByLayer" "_LA" "S_AXIS" "")
  (command "_chprop" azt_selekcja_as_grids "" "_LT" "ByLayer" "_C" "ByLayer" "_LA" "S_GRIDS" "")
  (command "_chprop" azt_selekcja_as_cyan "" "_LT" "ByLayer" "_C" "ByLayer" "_LA" "S_VIEW HIDDEN" "")
  (command "_chprop" azt_selekcja_as_green "" "_LT" "ByLayer" "_C" "ByLayer" "_LA" "S_VIEW SECTION" "")
  (command "_chprop" azt_selekcja_as_8 "" "_LT" "ByLayer" "_C" "ByLayer" "_LA" "S_HATCH" "")
  (command "_chprop" azt_selekcja_as_254 "" "_LT" "ByLayer" "_C" "ByLayer" "_LA" "S_VIEW" "")
  (print "Zakonczono podmiane warstw z Advance Steel")
)

