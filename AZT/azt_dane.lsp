  (command "_.CHPROP" stss "" "_LA" "S_HATCH" "")
  (setq stss (cadr (sssetfirst nil (ssget "_ALL" (list '(62 . 1) (cons 6 "ACAD_ISO02W100"))))))
  (command "_.CHPROP" stss "" "_LA" "S_VIEW HIDDEN" "")
  (command "_laymrg" "N" "ST_WIC" "" "N" "S_VIEW THIN" "Y")
